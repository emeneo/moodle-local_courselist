<div class="local_listcoursefiles_menu" style="margin:10px 0 10px 0;">
    <div class="local_listcoursefiles_selection">
        [<a href="{{addurl}}"><strong>{{btnAdd}}</strong></a>]
    </div>
</div>
<div class="hero-unit">
  <table class="local_listcoursefiles_table generaltable">
  {{#courses}}
    <tr>
      <td class="cell c1" style="text-align:left;">
        <a href="{{viewurl}}?id={{id}}">{{name}}</a>
      </td>
      <td class="cell c1" style="text-align:right;"><a href="{{addurl}}?id={{id}}">{{btnEdit}}</a>&nbsp;|&nbsp;<a href="{{delurl}}&id={{id}}">{{btnDel}}</a>&nbsp;|&nbsp;<a href="#" onclick="navigator.clipboard.writeText('{{viewurl}}?id={{id}}');alert('{{tipsCopy}}');">{{btnCopy}}</a></td>
    </tr>
    {{/courses}}
  </table>
</div>